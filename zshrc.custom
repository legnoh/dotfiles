# homebrew
export PATH="/usr/local/sbin:$PATH"

# brew-file
if [ -f $(brew --prefix)/etc/brew-wrap ];then
  source $(brew --prefix)/etc/brew-wrap
fi

# anyenv
export PATH="$HOME/.anyenv/bin:$PATH"
eval "$(anyenv init -)"

# go
export GOPATH="$HOME"

# ruby
export PATH="$HOME/.gem/ruby/2.3.0/bin:$PATH"

# setting
setopt nonomatch
